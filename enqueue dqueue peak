#include<stdio.h>
#define MAX 5

int queue[MAX];
int front = -1;
int rear  = -1;

void enqueue(int x){
    if(rear == MAX-1){
        printf("Queue overflow\n");
    }
    else{
        if(front == -1)
            front = 0;
        rear++;
        queue[rear] = x;
        printf("%d enqueued\n", x);
    }
}

void dequeue(){
    if(front == -1 || front > rear){
        printf("Queue Underflow\n");
    }
    else{
        printf("%d dequeued\n", queue[front]);
        front++;
    }
}

void peek(){
    if(front == -1 || front > rear){
        printf("Queue is empty\n");
    }
    else{
        printf("Front element is: %d\n", queue[front]);
    }
}

int main(){
    enqueue(10);
    enqueue(20);
    enqueue(30);

    peek();      // shows front element

    dequeue();
    peek();      // shows new front

    return 0;
}
